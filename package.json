{
  "name": "@flutchai/flutch-sdk",
  "version": "0.1.14",
  "type": "module",
  "description": "Base infrastructure package for building graph microservices - core interfaces, types, utilities, and execution engine",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./core": {
      "types": "./dist/core/index.d.ts",
      "default": "./dist/core/index.js"
    },
    "./callbacks": {
      "types": "./dist/callbacks/index.d.ts",
      "default": "./dist/callbacks/index.js"
    },
    "./graph": {
      "types": "./dist/graph/index.d.ts",
      "default": "./dist/graph/index.js"
    },
    "./engines": {
      "types": "./dist/engines/index.d.ts",
      "default": "./dist/engines/index.js"
    },
    "./agent-ui": {
      "types": "./dist/agent-ui/index.d.ts",
      "default": "./dist/agent-ui/index.js"
    },
    "./tools": {
      "types": "./dist/tools/index.d.ts",
      "default": "./dist/tools/index.js"
    },
    "./messages": {
      "types": "./dist/messages/index.d.ts",
      "default": "./dist/messages/index.js"
    },
    "./models": {
      "types": "./dist/models/index.d.ts",
      "default": "./dist/models/index.js"
    },
    "./retriever": {
      "types": "./dist/retriever/index.d.ts",
      "default": "./dist/retriever/index.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "default": "./dist/utils/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/flutchai/node-sdk.git"
  },
  "keywords": [
    "flutch",
    "langgraph",
    "nestjs",
    "ai",
    "microservices",
    "graph",
    "sdk",
    "langchain",
    "typescript"
  ],
  "author": "Flutchai",
  "license": "MIT",
  "scripts": {
    "build": "tsup",
    "test": "jest",
    "lint": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@langchain/anthropic": "^0.3.33",
    "@langchain/azure-openai": "^0.0.11",
    "@langchain/cohere": "^1.0.0",
    "@langchain/core": "^1.0.2",
    "@langchain/langgraph": "^1.0.1",
    "@langchain/mistralai": "^1.0.0",
    "@langchain/openai": "^1.0.0",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/mongoose": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/terminus": "^10.0.1",
    "@willsoto/nestjs-prometheus": "^5.0.0",
    "axios": "^1.6.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "ioredis": "^5.3.2",
    "mongoose": "^8.0.0",
    "prom-client": "^14.0.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "zod": "^3.22.4",
    "zod-from-json-schema": "^0.5.1"
  },
  "devDependencies": {
    "@jest/globals": "^29.0.0",
    "@types/express": "^4.17.17",
    "@types/ioredis-mock": "^8",
    "@types/jest": "^29.0.0",
    "@types/json-schema": "^7.0.15",
    "@types/node": "^20.3.1",
    "ioredis-mock": "^8.9.0",
    "jest": "^29.0.0",
    "json-schema": "^0.4.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.0.0",
    "tsup": "^8.5.0",
    "typescript": "^5.1.3"
  },
  "packageManager": "yarn@4.5.3"
}
